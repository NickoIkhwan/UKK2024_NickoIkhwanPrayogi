<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:backgroundTint="#17AEEE"
    tools:context=".MainActivity">

    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#110740">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/deadspace"
                android:layout_width="84dp"
                android:layout_height="220dp"
                android:layout_span="4"
                android:text="" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/tampil"
                android:layout_width="84dp"
                android:layout_height="41dp"
                android:layout_span="4"
                android:fontFamily="@font/cute_font"
                android:text=""
                android:textAlignment="textEnd"
                android:textColor="#009DFF"
                android:textSize="34sp" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/hasil"
                android:layout_width="84dp"
                android:layout_height="89dp"
                android:layout_span="4"
                android:fontFamily="@font/cute_font"
                android:text=""
                android:textAlignment="textEnd"
                android:textColor="#009DFF"
                android:textSize="100sp" />
        </TableRow>

        <TableRow
            android:layout_width="84dp"
            android:layout_height="match_parent">

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <Button
                android:id="@+id/sembilan"
                android:layout_width="84dp"
                android:layout_height="71dp"
                android:layout_marginLeft="5dp"
                android:backgroundTint="#004CFF"
                android:fontFamily="@font/cute_font"
                android:rotation="0"
                android:rotationX="0"
                android:rotationY="0"
                android:text="9"
                android:textColor="#FFFFFF"
                android:textSize="48sp"
                app:cornerRadius="10dp" />

            <Button
                android:id="@+id/delapan"
                android:layout_width="84dp"
                android:layout_height="71dp"
                android:layout_marginLeft="5dp"
                android:backgroundTint="#004CFF"
                android:fontFamily="@font/cute_font"
                android:rotation="0"
                android:rotationX="0"
                android:rotationY="0"
                android:text="8"
                android:textColor="#FFFFFF"
                android:textSize="48sp"
                app:cornerRadius="10dp" />

            <Button
                android:id="@+id/tujuh"
                android:layout_width="84dp"
                android:layout_height="71dp"
                android:layout_marginLeft="5dp"
                android:backgroundTint="#004CFF"
                android:fontFamily="@font/cute_font"
                android:rotation="0"
                android:rotationX="0"
                android:rotationY="0"
                android:text="7"
                android:textColor="#FFFFFF"
                android:textSize="48sp"
                app:cornerRadius="10dp" />

            <Button
                android:id="@+id/plus"
                android:layout_width="84dp"
                android:layout_height="71dp"
                android:layout_marginLeft="5dp"
                android:backgroundTint="#1212AF"
                android:fontFamily="@font/cute_font"
                android:rotation="0"
                android:rotationX="0"
                android:rotationY="0"
                android:text="+"
                android:textColor="#FFFFFF"
                android:textSize="48sp"
                app:cornerRadius="10dp" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <Button
                android:id="@+id/enam"
                android:layout_width="84dp"
                android:layout_height="71dp"
                android:layout_marginLeft="5dp"
                android:backgroundTint="#004CFF"
                android:fontFamily="@font/cute_font"
                android:rotation="0"
                android:rotationX="0"
                android:rotationY="0"
                android:text="6"
                android:textColor="#FFFFFF"
                android:textSize="48sp"
                app:cornerRadius="10dp" />

            <Button
                android:id="@+id/lima"
                android:layout_width="84dp"
                android:layout_height="71dp"
                android:layout_marginLeft="5dp"
                android:backgroundTint="#004CFF"
                android:fontFamily="@font/cute_font"
                android:rotation="0"
                android:rotationX="0"
                android:rotationY="0"
                android:text="5"
                android:textColor="#FFFFFF"
                android:textSize="48sp"
                app:cornerRadius="10dp" />

            <Button
                android:id="@+id/empat"
                android:layout_width="84dp"
                android:layout_height="71dp"
                android:layout_marginLeft="5dp"
                android:backgroundTint="#004CFF"
                android:fontFamily="@font/cute_font"
                android:rotation="0"
                android:rotationX="0"
                android:rotationY="0"
                android:text="4"
                android:textColor="#FFFFFF"
                android:textSize="48sp"
                app:cornerRadius="10dp" />

            <Button
                android:id="@+id/kali"
                android:layout_width="84dp"
                android:layout_height="71dp"
                android:layout_marginLeft="5dp"
                android:backgroundTint="#1212AF"
                android:fontFamily="@font/cute_font"
                android:rotation="0"
                android:rotationX="0"
                android:rotationY="0"
                android:text="*"
                android:textColor="#FFFFFF"
                android:textSize="48sp"
                app:cornerRadius="10dp" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <Button
                android:id="@+id/tiga"
                android:layout_width="84dp"
                android:layout_height="71dp"
                android:layout_marginLeft="5dp"
                android:backgroundTint="#004CFF"
                android:fontFamily="@font/cute_font"
                android:rotation="0"
                android:rotationX="0"
                android:rotationY="0"
                android:text="3"
                android:textColor="#FFFFFF"
                android:textSize="48sp"
                app:cornerRadius="10dp" />

            <Button
                android:id="@+id/dua"
                android:layout_width="84dp"
                android:layout_height="71dp"
                android:layout_marginLeft="5dp"
                android:backgroundTint="#004CFF"
                android:fontFamily="@font/cute_font"
                android:rotation="0"
                android:rotationX="0"
                android:rotationY="0"
                android:text="2"
                android:textColor="#FFFFFF"
                android:textSize="48sp"
                app:cornerRadius="10dp" />

            <Button
                android:id="@+id/satu"
                android:layout_width="84dp"
                android:layout_height="71dp"
                android:layout_marginLeft="5dp"
                android:backgroundTint="#004CFF"
                android:fontFamily="@font/cute_font"
                android:rotation="0"
                android:rotationX="0"
                android:rotationY="0"
                android:text="1"
                android:textColor="#FFFFFF"
                android:textSize="48sp"
                app:cornerRadius="10dp" />

            <Button
                android:id="@+id/bagi"
                android:layout_width="84dp"
                android:layout_height="71dp"
                android:layout_marginLeft="5dp"
                android:backgroundTint="#1212AF"
                android:fontFamily="@font/cute_font"
                android:rotation="0"
                android:rotationX="0"
                android:rotationY="0"
                android:text="/"
                android:textColor="#FFFFFF"
                android:textSize="48sp"
                app:cornerRadius="10dp" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <Button
                android:id="@+id/ttk"
                android:layout_width="84dp"
                android:layout_height="71dp"
                android:layout_marginLeft="5dp"
                android:backgroundTint="#004CFF"
                android:fontFamily="@font/cute_font"
                android:rotation="0"
                android:rotationX="0"
                android:rotationY="0"
                android:text="."
                android:textColor="#FFFFFF"
                android:textSize="48sp"
                app:cornerRadius="10dp" />

            <Button
                android:id="@+id/nol"
                android:layout_width="84dp"
                android:layout_height="71dp"
                android:layout_marginLeft="5dp"
                android:backgroundTint="#004CFF"
                android:fontFamily="@font/cute_font"
                android:rotation="0"
                android:rotationX="0"
                android:rotationY="0"
                android:text="0"
                android:textColor="#FFFFFF"
                android:textSize="48sp"
                app:cornerRadius="10dp" />

            <Button
                android:id="@+id/clr"
                android:layout_width="84dp"
                android:layout_height="71dp"
                android:layout_marginLeft="5dp"
                android:layout_span="0"
                android:backgroundTint="#1212AF"
                android:fontFamily="@font/cute_font"
                android:rotation="0"
                android:rotationX="0"
                android:rotationY="0"
                android:text="C"
                android:textColor="#FFFFFF"
                android:textSize="48sp"
                app:cornerRadius="10dp" />

            <Button
                android:id="@+id/min"
                android:layout_width="84dp"
                android:layout_height="71dp"
                android:layout_marginLeft="5dp"
                android:layout_span="2"
                android:backgroundTint="#1212AF"
                android:fontFamily="@font/cute_font"
                android:rotation="0"
                android:rotationX="0"
                android:rotationY="0"
                android:text="-"
                android:textColor="#FFFFFF"
                android:textSize="48sp"
                app:cornerRadius="10dp" />


        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <Button
                android:id="@+id/sd"
                android:layout_width="84dp"
                android:layout_height="71dp"
                android:layout_marginLeft="5dp"
                android:layout_span="4"
                android:backgroundTint="#1212AF"
                android:fontFamily="@font/cute_font"
                android:rotation="0"
                android:rotationX="0"
                android:rotationY="0"
                android:text="="
                android:textColor="#FFFFFF"
                android:textSize="48sp"
                app:cornerRadius="10dp" />

        </TableRow>
    </TableLayout>
</androidx.constraintlayout.widget.ConstraintLayout>